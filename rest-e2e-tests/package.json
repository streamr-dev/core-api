    "mocha": "8.4.0",
    "node-fetch": "2.6.1",
    "sleep-promise": "9.1.0",
    "streamr-client": "5.3.0-alpha.3",
    "ts-mocha": "8.0.0",
    "typescript": "4.2.4"
  },
