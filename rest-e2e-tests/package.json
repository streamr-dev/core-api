  "engines": {
    "node": ">=14",
    "npm": ">=7.13"
  },
  "dependencies": {
    "@ethersproject/wallet": "5.4.0",
    "ajv": "8.6.3",
    "chai": "4.3.4",
    "form-data": "4.0.0",
    "lodash": "4.17.21",
    "mocha": "8.4.0",
    "node-fetch": "3.0.0",
    "streamr-client": "5.4.4",
    "ts-mocha": "8.0.0",
    "typescript": "4.4.3"
  },
  "devDependencies": {
    "@types/chai": "4.2.21",
    "@types/lodash": "4.14.173",
    "@types/mocha": "9.0.0",
    "@types/node": "16.9.4",
    "@types/node-fetch": "3.0.3"
  }
