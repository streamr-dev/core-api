  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": "https://github.com/streamr-dev/engine-and-editor",
  "scripts": {
    "test": "npm run test-old; npm run test-new",
    "test-old": "mocha web-app/js/unifina/test --recursive",
    "test-new": "node ./node_modules/.bin/jest ./web-app/react-app/tests",
    "test-jenkins": "npm run test-old-jenkins; npm run test-new-jenkins",
    "test-old-jenkins": "mocha web-app/js/unifina/test --recursive --reporter mocha-junit-reporter --reporter-options mochaFile=./test-old-report.xml",
    "test-new-jenkins": "JEST_JUNIT_OUTPUT=test-new-report.xml jest ./web-app/react-app/tests --ci --testResultsProcessor=\"jest-junit\"",
    "build": "NODE_ENV=production webpack",
    "build-dev": "webpack",
    "clean": "rm -rf node_modules/",
    "clean-install": "npm run clean && npm install",
    "dev": "webpack-dev-server --hot"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|pcss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./web-app/react-app/tests/setup-tests.js"
    ]
  },
